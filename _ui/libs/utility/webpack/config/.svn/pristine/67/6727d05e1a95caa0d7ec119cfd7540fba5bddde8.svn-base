{
    "presets": [
        ["@babel/preset-env", {
            "modules": false,
            "targets": {
                "browsers": [
                    "defaults",
                    "not < 1%",
                    "IE 11",
                    "not op_mini all"
                ]
            },
            "useBuiltIns": "usage"
        }]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime"],
        ["@babel/plugin-proposal-object-rest-spread", {"useBuiltIns": true}],
        ["@babel/plugin-transform-object-assign"]
    ]
}
